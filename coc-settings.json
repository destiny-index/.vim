{
  "diagnostic.displayByAle": true,
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": [
        "--lsp"
      ],
      "filetypes": [
        "hs",
        "lhs",
        "haskell"
      ]
    },
    "godot": {
      "host": "127.0.0.1",
      "filetypes": ["gdscript"],
      "port": 6008
    }
  }
}
